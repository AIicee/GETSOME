<Window x:Class="GETSOME.KontaktNoteWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GETSOME"
        mc:Ignorable="d"
        Title="Kontakt Kunde" Height="200" Width="400">
	<Grid>
		<Button x:Name="KontaktetOK" Content="OK" HorizontalAlignment="Left" Margin="90,0,0,20" VerticalAlignment="Bottom" Width="75" Height="25" Click="KontaktetOK_Click"/>
		<Button x:Name="KontaktetAnnuller" Content="Cancel" HorizontalAlignment="Right" Margin="0,0,90,20" VerticalAlignment="Bottom" Width="75" Height="25" Click="KontaktetAnnuller_Click"/>
		<TextBox x:Name="KontaktetNoteBox" HorizontalAlignment="Right" Height="23" Margin="0,68,50,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="237"/>
		<TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="50,70,0,0" TextWrapping="Wrap" Text="Note:" VerticalAlignment="Top" Width="50" Height="17"/>
		<TextBlock x:Name="textBlock1" HorizontalAlignment="Center" Margin="0,25,0,0" TextWrapping="Wrap" Text="Kunde: " VerticalAlignment="Top" FontSize="18"/>

	</Grid>
</Window>
